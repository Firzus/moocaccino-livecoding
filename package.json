{
  "name": "ts-jasmine-webpack-template",
  "version": "1.0.0",
  "description": "A template for typescript jasmine webpack projects",
  "main": "dist/lib/index.js",
  "type": "dist/lib/index.ts",
  "scripts": {
    "start": "nodemon -e ts --watch lib --exec \"npm run build\"",
    "build": "tsc && npm run pack && http-server dist",
    "test": "tsc && jasmine",
    "pack": "npx webpack --config webpack.config.js",
    "jasminit": "jasmine init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thetinyspark/ts-jasmine-webpack-template.git"
  },
  "keywords": [
    "typescript",
    "jasmine",
    "webpack"
  ],
  "author": "The Tiny Spark <contact@the-tiny-spark.com> (http://the-tiny-spark.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thetinyspark/ts-jasmine-webpack-template/issues"
  },
  "homepage": "https://github.com/thetinyspark/ts-jasmine-webpack-template#readme",
  "dependencies": {
    "babylonjs-viewer": "^4.2.0",
    "babylonjs": "^4.2.0",
    "babylonjs-materials": "^4.2.0",
    "rxjs": "^6.6.6"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "jasmine": "^3.5.0",
    "nodemon": "^2.0.7",
    "typedoc": "^0.20.32",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.12"
  }
}
